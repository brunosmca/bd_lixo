<h1>Listing usuarios</h1>

<table>
  <tr>
    <th>Login</th>
    <th>Password</th>
    <th>Nome</th>
    <th>Sexo</th>
    <th>Data nascimento</th>
    <th>Endereco</th>
    <th>Telefone</th>
    <th>Data contratacao</th>
    <th>Salario</th>
    <th>Habilidades</th>
    <th>Administrador</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @usuarios.each do |usuario| %>
  <tr>
    <td><%= usuario.login %></td>
    <td><%= usuario.password %></td>
    <td><%= usuario.nome %></td>
    <td><%= usuario.sexo %></td>
    <td><%= usuario.data_nascimento %></td>
    <td><%= usuario.endereco %></td>
    <td><%= usuario.telefone %></td>
    <td><%= usuario.data_contratacao %></td>
    <td><%= usuario.salario %></td>
    <td><%= usuario.habilidades %></td>
    <td><%= usuario.administrador %></td>
    <td><%= link_to 'Show', usuario %></td>
    <td><%= link_to 'Edit', edit_usuario_path(usuario) %></td>
    <td><%= link_to 'Destroy', usuario, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Usuario', new_usuario_path %>
